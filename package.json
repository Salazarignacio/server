{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon ./server.js -p 8080  --env=dev ",
    "prod": "nodemon ./server.js -p 8080  --env=prod  ",
    "test": "nodemon ./server.js -p 8081 --persistence=fs",
    "argv": "nodemon ./server.js -p 8080  --env=prod --persistence=fs",
    "memory": "nodemon ./server.js -p 8080 --env=dev --persistence=memory",
    "testing": "node ./test/test.js",
    "mock_products": "node ./src/mocks/products.mock.js",
    "mock_users": "node ./src/mocks/users.mock.js",
    "uu": "node ./src/mocks/updateUsers.js",
    "simplex": "artillery quick --count 20 --num 100 \"http://localhost:8080/api/artillery/simplex\" -o ./test/artillery/simplex.json",
    "complex": "artillery quick --count 20 --num 100 \"http://localhost:8080/api/artillery/complex\" -o ./test/artillery/complex.json",
    "flow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "report": "artillery report ./test/artillery/flow.json",
    "image": "docker build -t server .",
    "container": "docker run -p 8888:8080 server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "nodemon": "^3.1.4"
  },
  "dependencies": {
    "artillery": "^2.0.17",
    "artillery-plugin-metrics-by-endpoint": "^1.11.0",
    "bcrypt": "^5.1.1",
    "commander": "^12.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parse": "^0.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "joi": "^17.13.3",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.4",
    "mongoose-paginate-v2": "^1.8.2",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1"
  }
}
